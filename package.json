{
   "version": "0.0.0",
   "private": true,
   "engines": {
      "node": ">=14.17.0 < 17.0.0",
      "npm": "~6.0.0"
   },
   "source": true,
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/express": "^4.17.11",
      "@types/jest": "^26.0.23",
      "@types/lodash": "^4.14.173",
      "@types/multer": "^1.4.5",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^4.22.1",
      "@typescript-eslint/parser": "^4.22.1",
      "eslint": "^7.25.0",
      "jest": "^26.6.3",
      "nodemon": "^2.0.7",
      "supertest": "^6.1.3",
      "ts-jest": "^26.5.5",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.1.8",
      "typeorm-seeding": "^1.6.1",
      "typescript": "^4.4.3"
   },
   "dependencies": {
      "axios": "^0.21.1",
      "bignumber.js": "^9.0.1",
      "body-parser": "^1.19.0",
      "class-transformer": "^0.3.1",
      "class-validator": "^0.12.2",
      "dayjs": "^1.10.5",
      "dotenv": "^8.4.0",
      "ethers": "^5.4.7",
      "express": "^4.17.1",
      "lodash": "^4.17.21",
      "multer": "^1.4.2",
      "mysql2": "^2.3.0",
      "pg": "^8.6.0",
      "pinpoint-node-agent": "^0.8.2",
      "reflect-metadata": "^0.1.10",
      "routing-controllers": "^0.9.0",
      "save": "^2.4.0",
      "typedi": "0.8.0",
      "typeorm": "^0.2.37",
      "typeorm-naming-strategies": "^2.0.0",
      "typeorm-typedi-extensions": "^0.4.1",
      "winston": "^3.3.3"
   },
   "scripts": {
      "start": "ts-node src/start",
      "local": "ts-node-dev --respawn --transpile-only src/start.ts PINPOINT_AGENT_ID=local",
      "test": "jest --runInBand",
      "test:unit": "jest -c=jest.config.unit.js",
      "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f src/config/database.ts",
      "migration:create": "npm run typeorm:cli -- migration:create -n",
      "migration:generate": "npm run typeorm:cli -- migration:generate -n",
      "migration:run": "npm run typeorm:cli -- migration:run",
      "typeorm-seeding:cli": "ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName src/config/database.seed.ts",
      "seed:run": "npm run typeorm-seeding:cli seed"
   }
}
